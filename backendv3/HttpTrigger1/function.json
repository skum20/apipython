{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post",
        "delete",
        "put"
      ],
      "route": "{param1}/{param2?}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]}

